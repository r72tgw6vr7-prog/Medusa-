{
  "name": "medusa-tattoo",
  "version": "1.0.0",
  "description": "Medusa Tattoo München - Premium Tattoo Studio Website",
  "main": "src/index.tsx",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:local": "npm run optimize-images && vite build",
    "build:force": "SKIP_VERIFY=true vite build",
    "preview": "vite preview",
    "analyze": "vite build && open dist/stats.html",
    "start": "vite dev",
    "prepare-deploy": "npm run optimize-images && npm run build",
    "deploy:preview": "npm run prepare-deploy && vercel",
    "deploy:production": "npm run prepare-deploy && vercel --prod",
    "optimize-images:old": "node scripts/optimize-images.mjs",
    "lint": "eslint \"**/*.{ts,tsx}\" --config eslint.config.js",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --config eslint.config.js --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "cleanup": "npm run lint:fix && npm run format",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "optimize-images": "node scripts/optimize-images.cjs",
    "verify-images": "node scripts/verify-images.cjs",
    "convert-to-webp": "node scripts/convert-to-webp.js",
    "seo-audit": "node scripts/seo-audit.cjs"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "tattoo",
    "münchen",
    "website",
    "luxury",
    "react"
  ],
  "author": "Medusa Tattoo München",
  "license": "MIT",
  "dependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-aria/button": "^3.14.2",
    "@react-aria/focus": "^3.21.2",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "scheduler": "^0.23.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^2.15.2",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.3.1",
    "terser": "^5.44.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@playwright/test": "^1.56.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.4",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0",
    "vite": "^7.1.12",
    "vite-plugin-compression": "^0.5.1"
  }
}
