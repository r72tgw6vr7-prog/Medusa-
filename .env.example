# Copy this file to .env and fill in your actual values

# Google Maps API Key (for Contact page map)
# Get your API key from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Analytics 4 Measurement ID
# Format: G-XXXXXXXXXX
VITE_GA4_MEASUREMENT_ID=your_ga4_measurement_id_here

# Environment
VITE_APP_ENV=development

# Contact Form Email Service (if using a service like EmailJS)
VITE_EMAIL_SERVICE_ID=your_email_service_id
VITE_EMAIL_TEMPLATE_ID=your_email_template_id
VITE_EMAIL_PUBLIC_KEY=your_email_public_key

# Optional: Custom domain for production
VITE_DOMAIN=https://yourdomain.com